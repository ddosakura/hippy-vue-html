{
  "name": "hippy-vue-html",
  "version": "0.0.4",
  "description": "A rich text renderer for hippy-vue",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:hippy-contrib/hippy-vue-html.git"
  },
  "author": "kiritocui",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "npx husky install",
    "dev": "rollup --watch --config rollup.config.js",
    "build": "rollup --config rollup.config.js",
    "build:version": "yarn version --patch --message \"chore(release): publish v%s\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "pub": "yarn build && yarn build:version && npm publish && git push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-core": "^6.26.3",
    "babel-eslint-parser": "^7.13.10",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "core-js": "^3.21.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-tencent": "^1.0.3",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rollup-plugin-babel": "3.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^6.0.0"
  }
}
